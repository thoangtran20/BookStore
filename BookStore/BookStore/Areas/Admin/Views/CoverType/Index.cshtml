@model IEnumerable<CoverType>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Cover Type List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="CoverType" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Create New Cover Type
            </a>
        </div>
    </div>
    <br />
    <br />
    <table class="table table-bordered table-striped" id="tblData">
        <thead>
            <th>CoverType Name</th>
            <th></th>
        </thead>
    </table>
</div>


@section Scripts{
    <script src="~/js/coverType.js"></script>
}

